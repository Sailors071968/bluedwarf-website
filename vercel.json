{
    "version": 2,
    "builds": [
          {
                  "src": "api/index.py",
                  "use": "@vercel/python"
          }
    ],
    "routes": [
          {
                  "src": "/api/(.*)",
                  "dest": "/api/index.py"
          },
          {
                  "src": "/static/(.*)",
                  "dest": "/src/static/$1"
          },
          {
                  "src": "/(.*)",
                  "dest": "/api/index.py"
          }
    ],
    "env": {
          "GOOGLE_PLACES_API_KEY": "AIzaSyDe8QxfkBSo2Ids9PWK24-aKgqbI9du9B4"
    }
}
