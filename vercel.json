{
  "version": 2,
  "builds": [
    {
      "src": "*.html",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/about",
      "dest": "/about.html"
    },
    {
      "src": "/contact", 
      "dest": "/contact.html"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}
